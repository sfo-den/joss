<div class="editors">
  <div class="content">
    <h1>Editing</h1>
    <%= render 'form' %>

    <div class="links">
      <%= link_to 'Show', @editor %> |
      <% if @editor.pending? %>
        <%= link_to 'List of pending editors', onboardings_path(anchor: 'editors') %>
      <% else %>
        <%= link_to 'List', editors_path %>
      <% end %>
    </div>
  </div>
</div>
